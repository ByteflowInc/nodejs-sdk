{
	"name": "@byteflow-inc/sdk",
	"version": "1.4.0",
	"description": "This is an SDK for Byteflow. An SMS messaging provider that let's you start sending messages without the wait.",
	"keywords": [
		"sms",
		"messaging",
		"sdk",
		"byteflow"
	],
	"homepage": "https://github.com/ByteflowInc/sdk",
	"repository": {
		"type": "git",
		"url": "https://github.com/ByteflowInc/sdk.git"
	},
	"license": "MIT",
	"author": "Bryght, LLC",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "tslint -p tsconfig.json",
		"prepare": "npm run build",
		"version": "npm run format && git add -A src"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"axios-retry": "^3.9.1",
		"nanoid": "^5.0.3"
	},
	"devDependencies": {
		"@types/node": "^20.9.2",
		"@vercel/style-guide": "^5.1.0",
		"prettier": "^3.1.0",
		"prettier-plugin-packagejson": "^2.4.6",
		"typescript": "^5.2.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
